{
  automerge: true,
  extends: [
    "config:base",
    "github>suzuki-shunsuke/aqua-renovate-config",
  ],
  regexManagers: [
    {
      fileMatch: ["README.md"],
      matchStrings: [
        "ref: (?<currentValue>.*?) # renovate: depName=(?<depName>.*)",
      ],
      datasourceTemplate: "github-releases",
    },
    {
      fileMatch: [".github/workflows/test.yaml"],
      matchStrings: [
        "version: (?<currentValue>.*?) # renovate: depName=(?<depName>.*)",
      ],
      datasourceTemplate: "github-releases",
    },
    {
      fileMatch: ["CONTRIBUTING\\.md"],
      matchStrings: [
        " +(version|ref): (?<currentValue>[^'\"\\n]*) +# renovate: depName=(?<depName>[^\\n]+)",
        " +(version|ref): \"(?<currentValue>[^'\"\\n]*)\" +# renovate: depName=(?<depName>[^\\n]+)",
        " +(version|ref): '(?<currentValue>[^'\"\\n]*)' +# renovate: depName=(?<depName>[^\\n]+)",
        " +name: (?<depName>[^'\"\\n]*)@(?<currentValue>[^'\"\\n]+)",
        " +name: \"(?<depName>[^'\\n]*)@(?<currentValue>[^'\"\\n]+)\"",
        " +name: '(?<depName>[^\"\\n]*)@(?<currentValue>[^'\"\\n]+)'"
      ],
      datasourceTemplate: "github-releases",
    },
  ],
}
